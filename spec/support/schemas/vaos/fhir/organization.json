{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "resourceType",
    "id",
    "text",
    "identifier",
    "active",
    "name",
    "address"
  ],
  "properties": {
    "resourceType": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "text": {
      "type": "object",
      "default": {},
      "required": [
        "status",
        "div"
      ],
      "properties": {
        "status": {
          "type": "string"
        },
        "div": {
          "type": "string"
        }
      }
    },
    "identifier": {
      "type": "array",
      "items": {
        "$id": "#/properties/identifier/items/anyOf/0",
        "type": "object",
        "title": "The first anyOf schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
          {
            "value": "580",
            "system": "urn:oid:2.16.840.1.113883.6.233"
          }
        ],
        "required": [
          "system",
          "value"
        ],
        "properties": {
          "system": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "active": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "address": {
      "type": "array",
      "items": {
        "$id": "#/properties/address/items/anyOf/0",
        "type": "object",
        "title": "The first anyOf schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
          {
            "postalCode": "77030-4298",
            "city": "Houston",
            "line": [
              "2002 Holcombe Blvd."
            ],
            "state": "TX"
          }
        ],
        "required": [
          "line",
          "city",
          "state",
          "postalCode"
        ],
        "properties": {
          "line": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "city": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "postalCode": {
            "type": "string"
          }
        }
      }
    }
  }
}
