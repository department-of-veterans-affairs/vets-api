{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "type" : "object",
  "required": ["branch_of_service", "start_date", "end_date"],
  "properties": {
    "branch_of_service": { "type": "string", "pattern": "^\\w{2}$" },
    "start_date": { "type": "string", "pattern": "^\\d{4}-(0[1-9]|1[012])-([012][0-9]|3[01])$" },
    "end_date": { "type": ["string", "null"], "pattern": "^(\\d{4}-(0[1-9]|1[012])-([012][0-9]|3[01])|null)$" }
  }
}
