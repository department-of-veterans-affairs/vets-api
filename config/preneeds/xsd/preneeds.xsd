			<xs:element name="attachmentType" type="tns:attachmentType" />
			<xs:element name="dataHandler" type="xs:base64Binary" />
			<xs:element name="description" type="xs:string" />
			<xs:element name="sendingName" >
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[a-zA-Z0-9_+-]+\.pdf" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sendingSource">
