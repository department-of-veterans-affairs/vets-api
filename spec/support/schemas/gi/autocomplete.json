{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "meta": {
      "type": "object",
      "properties": {
        "version": { "type": "integer" },
        "term": { "type": "string" }
      }
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": { "type": "integer" },
          "value": { "type": "string" },
          "label": { "type": "string" }
        }
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "self": { "type": "string" }
      }
    }
  },
  "required": [
    "meta",
    "data",
    "links"
  ]
}
