{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "items": {
        "required": [
          "id",
          "type",
          "attributes"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "enum": [
              "preneeds_attachment_types"
            ]
          },
          "attributes": {
            "type": "object",
            "required": [
              "attachmentTypeId",
              "description"
            ],
            "properties": {
              "attachmentTypeId": {
                "type": "integer"
              },
              "description": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
