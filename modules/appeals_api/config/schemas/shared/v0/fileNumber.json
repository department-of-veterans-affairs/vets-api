{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "JSON Schema for VA Appeal Forms: 'fileNumber'",
  "properties": {
    "fileNumber": {
      "description": "Veteran's VA file number (c-file / css)",
      "type": "string",
      "minLength": 7,
      "maxLength": 9,
      "example": "987654321"
    }
  }
}
