{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Veteran Identifier",
  "type": "object",
  "required": ["ssn", "birthdate", "firstName", "lastName"],
  "properties": {
    "ssn": {
      "type": "string",
      "example": "796130115",
      "description": "SSN of Veteran being represented"
    },
    "firstName": {
      "type": "string",
      "example": "Tamara",
      "description": "First Name of Veteran being represented"
    },
    "lastName": {
      "type": "string",
      "example": "Ellis",
      "description": "Last Name of Veteran being represented"
    },
    "birthdate": {
      "type": "string",
      "example": "1967-06-19",
      "description": "Date of Birth of Veteran being represented, in iso8601 format"
    }
  }
}
