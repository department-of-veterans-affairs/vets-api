{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Form 0966 Schema",
  "type": "object",
  "required": ["type"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["compensation", "pension", "burial"]
    },
    "participant_claimant_id": {
      "type": "string",
      "example": "123456789"
    },
    "participant_vet_id": {
      "type": "string",
      "example": "987654321"
    },
    "received_date": {
      "type": "string",
      "example": "2015-01-05T17:42:12.058Z",
      "format": "datetime"
    }
  }
}
