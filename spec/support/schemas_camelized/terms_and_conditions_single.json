            "name": {
              "type": "string"
            },
            "version": {
              "type": "string"
            },
            "createdAt": {
              "type": "string",
              "format": "date"
            },
            "updatedAt": {
              "type": "string",
              "format": "date"
            },
            "headerContent": {
              "type": "string"
            },
            "termsContent": {
              "type": "string"
            },
            "yesContent": {
              "type": "string"
            },
            "noContent": {
              "type": "string"
            },
            "footerContent": {
              "type": "string"
            },
            "title": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
