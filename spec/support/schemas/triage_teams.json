{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "type" : "object",
  "required": ["data", "meta"],
  "properties": {
    "data" : {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "required": ["id", "type", "attributes"],
        "properties": {
          "id": { "type": "string"},
          "type": { "enum": ["triage_teams"] },
          "attributes": {
            "type": "object",
            "$ref": "triage_team.json"
          }
        }
      }
    },
    "meta": {
      "type": "object",
      "required": ["sort"],
      "properties": {
        "sort": {
          "type": "object"
        }
      }
    }
  }
}
