{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Veteran Identifier",
  "type": "object",
  "required": [
    "ssn",
    "firstName",
    "lastName",
    "birthdate"
  ],
  "properties": {
    "ssn": {
      "type": "string",
      "pattern": "^\\d{9}$"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "birthdate": {
      "type": "string",
      "format": "date"
    }
  }
}
