{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "required": [
    "decision_code",
    "decision_text",
    "diagnostic_code",
    "name",
    "effective_date",
    "rated_disability_id",
    "rating_decision_id",
    "rating_percentage",
    "related_disability_date",
    "special_issues"
  ],
  "properties": {
    "decision_code": {
      "type": "string"
    },
    "decision_text": {
      "type": "string"
    },
    "diagnostic_code": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "effective_date": {
      "type": "datetime"
    },
    "rated_disability_id": {
      "type": "string"
    },
    "rating_decision_id": {
      "type": "string"
    },
    "rating_percentage": {
      "type": "integer"
    },
    "maximum_rating_percentage": {
      "type": ["integer", "null"]
    },
    "related_disability_date": {
      "type": "datetime"
    },
    "special_issues": {
      "type": "array"
    }
  },
  "type": "object"
}

