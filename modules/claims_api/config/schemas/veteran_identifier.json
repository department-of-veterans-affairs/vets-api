{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Veteran Identifier",
  "type": "object",
  "required": [
    "veteranSSN",
    "veteranFirstName",
    "veteranLastName",
    "veteranBirthDate"
  ],
  "properties": {
    "veteranSSN": {
      "type": "string"
    },
    "veteranFirstName": {
      "type": "string"
    },
    "veteranLastName": {
      "type": "string"
    },
    "veteranBirthDate": {
      "type": "string",
      "format": "date"
    }
  }
}
