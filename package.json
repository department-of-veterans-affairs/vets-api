{
  "name": "vets-api-medical-records-test-server",
  "version": "1.0.0",
  "description": "Medical Records FHIR pagination test server for vets-api",
  "main": "scripts/medical_records_listener.js",
  "scripts": {
    "start": "node scripts/medical_records_listener.js",
    "test:errors": "RETURN_ERROR=true node scripts/medical_records_listener.js",
    "test:timeout": "RETURN_ERROR=true ERROR_TYPE=timeout node scripts/medical_records_listener.js",
    "test:server-error": "RETURN_ERROR=true ERROR_TYPE=server_error node scripts/medical_records_listener.js",
    "test:nil-resource": "RETURN_ERROR=true ERROR_TYPE=nil_resource node scripts/medical_records_listener.js",
    "quiet": "LOG_REQUESTS=false node scripts/medical_records_listener.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/department-of-veterans-affairs/vets-api.git"
  },
  "keywords": [
    "medical-records",
    "fhir",
    "pagination",
    "testing",
    "va"
  ],
  "license": "CC0-1.0"
}
