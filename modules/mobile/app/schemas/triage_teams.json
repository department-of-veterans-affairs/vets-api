            "type": "boolean"
          },
          "group_type_enum_val": {
            "type": ["string", "null"]
          },
          "sub_group_type_enum_val": {
            "type": ["string", "null"]
          },
          "group_type_patient_display": {
            "type": ["string", "null"]
          },
          "sub_group_type_patient_display": {
            "type": ["string", "null"]
