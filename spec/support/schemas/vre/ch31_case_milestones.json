{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["id", "type", "attributes"],
      "properties": {
        "id": { "type": "string" },
        "type": { "type": "string" },
        "attributes": {
          "type": "object",
          "required": ["res_case_id", "response_message"],
          "properties": {
            "res_case_id": { "type": ["integer", "null"] },
            "response_message": { "type": ["string", "null"] }
          }
        }
      }
    }
  }
}
