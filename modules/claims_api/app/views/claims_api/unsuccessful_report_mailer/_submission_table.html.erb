<table>
  <thead>
    <tr>
      <th>GUID</th>
      <th>Consumer Name</th>
      <th>Status</th>
      <th>EVSS Response</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>
  <tbody>
    <% claims.each do |claim| %>
      <tr>
        <td><%= claim.id %></td>
        <td><%= claim.source %></td>
        <td><%= claim.status %></td>
        <td><%= claim.evss_response&.to_s %></td>
        <td><%= claim.created_at %></td>
        <td><%= claim.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
