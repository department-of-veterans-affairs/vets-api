			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Only run the number of pings that the num_webhook_urls specifies\r",
							"if(pm.environment.get(\"num_webhook_urls\") == 1) {\r",
							"    postman.setNextRequest(null);\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "raw",
