{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "data",
    "meta"
  ],
  "properties": {
    "data": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "prescription_details.json"
      }
    },
    "meta": {
      "type": "object",
      "required": [
        "updatedAt",
        "failedStationList",
        "sort"
      ],
      "properties": {
        "updatedAt": {
          "type": "string"
        },
        "failedStationList": {
          "type": "string"
        },
        "sort": {
          "type": "object"
        },
        "filter": {
          "type": "object"
        }
      }
    }
  }
}
