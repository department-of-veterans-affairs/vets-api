{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "identifier",
          "kind",
          "status",
          "location_id",
          "clinic",
          "start",
          "cancellable",
          "extension"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string",
            "maxLength": 64
          },
          "identifier": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "system",
                "value"
              ],
              "properties": {
                "system": { "type": "string" },
                "value": { "type": "string" }
              }
            }
          },
          "kind": {
            "type": ["string", "null"], // null not explicitly mentioned in docs
            "enum": ["clinic", "cc", "telehealth", "phone"]
          },
          "status": {
            "type": ["string", "null"], // null not mentioned in docs
            "enum": ["proposed", "pending", "booked", "arrived", "noshow", "fulfilled", "cancelled", "entered-in-error", "checked-in"]
          },
          "serviceType": { "type": "string" },
          "serviceTypes": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "coding": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "system": { "type": "string" },
                      "code": { "type": "string" },
                      "display:": { "type": "string" }
                    }
                  }
                },
                "text": { "type": "string" }
              }
            }
          },
          "serviceCategory": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "coding": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "system": { "type": "string" },
                      "code": { "type": "string" },
                      "display:": { "type": "string" }
                    }
                  }
                },
                "text": { "type": "string" }
              }
            }
          },
          "reasonCode": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "coding": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "system": { "type": "string" },
                      "code": { "type": "string" },
                      "display:": { "type": "string" }
                    }
                  }
                },
                "text": { "type": "string" }
              }
            }
          },
          "description": { "type": "string" },
          "priority": { "type": "number" },
          "patientIcn": {
            "type": "string",
            "nullable": false
          },
          "locationId": { "type": "string" },
          "clinic": { "type": "string" },
          "practitionerIds": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [ "system", "type" ],
              "properties": {
                "system": { "type": "string" },
                "type": { "type": "string" }
              }
            }
          },
          "practitioners": {
            "type": "array",
            "items": {
              "type": "array",
              "properties": {
                "identifier": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": ["system", "value"],
                    "properties": {
                      "system": { "type": "string" },
                      "value": {"type": "string"}
                    }
                  }
                },
                "name": {
                  "type": "object",
                  "properties": {
                    "family": {"type": "string"},
                    "given": {
                      "type": "array",
                      "items": { "type": "string" }
                    }
                  }
                },
                "practiceName": { "type": "string" },
                "address": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": ["postal", "physical", "both"]
                    },
                    "line": {
                      "type": "array",
                      "items": { "type": "string" }
                    },
                    "city": { "type": "string" },
                    "state": { "type": "string" },
                    "postalCode": { "type": "string" },
                    "country": { "type": "string" },
                    "text": { "type": "string" }
                  }
                },
                "firstName": { "type": "string" },
                "lastName": { "type": "string" }
              }
            }
          },
          "reason": { "type": "string" },
          "start": { "type": "string" }, // format datetime?
          "end": { "type": "string" }, //format datetime?
          "minutesDuration": { "type": "integer" },
          "slot": {
            "type": "object",
            "properties": {
              "id": { "type": "string" }, //pattern
              "start": { "type": "string" },
              "end": { "type": "string" }
            }
          },
          "created": { "type": "string" },
          "requestedPeriods": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "start": { "type": "string" },
                "end": { "type": "string" }
              }
            }
          },
          "contact": {
            "type": "object",
            "properties": {
              "telecom": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "nullable": false,
                      "enum": [ "email", "phone"]
                    },
                    "value": {
                      "type": "string",
                      "nullable": false
                    }
                  }
                }
              }
            }
          },
          "preferredTimesForPhoneCall": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["Morning", "Afternoon", "Evening"]
            }
          },
          "preferredLocation": {
            "type": "object",
            "properties": {
              "city": { "type": "string" },
              "state": { "type": "string" }
            }
          },
          "cancelationReason": {
            "type": "object",
            "properties": {
              "properties": {
                "system": { "type": "string" },
                "code": { "type": "string" },
                "display": { "type": "string" }
              }
            }
          },
          "comment": { "type": "string" },
          "preferredLanguage": { "type": "string" },
          "cancellable": { "type": "boolean" },
          "patientInstruction": { "type": "string" },
          "telehealth": {
            "type": "object",
            "properties": {
              "url": { "type": "string" },
              "atlas": {
                "type": "object",
                "properties": {
                  "sitecode": { "type": "string" },
                  "confirmationCode": { "type": "string" },
                  "address": {
                    "type": "object",
                    "properties": {
                      "streetAddress": { "type": "string" },
                      "city": { "type": "string" },
                      "state": { "type": "string" },
                      "zipCode": { "type": "string" },
                      "country": { "type": "string" },
                      "latitude": { "type": "number" },
                      "longitude": { "type": "number" },
                      "additionalDetails": { "type": "string" }
                    }
                  }
                }
              },
              "group": { "type": "boolean" },
              "vvs_kind": { "type": "string" }
            }
          },
          "extension": {
            "type": "object",
            "properties": {
              "ccLocation": {
                "type": "object",
                "properties": {
                  "address": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": ["postal", "physical", "both"]
                      },
                      "line": {
                        "type": "array",
                        "items": { "type": "string" }
                      },
                      "city": { "type": "string" },
                      "state": { "type": "string" },
                      "postalCode": { "type": "string" },
                      "country": { "type": "string" },
                      "text": { "type": "string" }
                    }
                  },
                  "telecom": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "system": {
                          "type": "string",
                          "enum": ["email", "phone"]
                        },
                        "value": { "type": "string" }
                      }
                    }
                  },
                  "practiceName": { "type": "string" }
                }
              }
            }
          }
        }
      }
    }
  }
}