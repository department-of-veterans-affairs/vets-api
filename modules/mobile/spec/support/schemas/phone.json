{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "required": [
    "id",
    "areaCode",
    "countryCode",
    "extension",
    "phoneNumber",
    "phoneType"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "areaCode": {
      "type": "string"
    },
    "countryCode": {
      "type": "string"
    },
    "extension": {
      "type": ["string", "null"]
    },
    "phoneNumber": {
      "type": "string"
    },
    "phoneType": {
      "type": "string",
      "enum": ["FAX", "HOME", "MOBILE", "WORK"]
    }
  }
}
