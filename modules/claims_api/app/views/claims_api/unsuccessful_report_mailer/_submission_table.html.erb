<table>
  <thead>
    <tr>
      <th>Consumer Name</th>
      <th>Status</th>
      <th>GUID</th>
    </tr>
  </thead>
  <tbody>
    <% claims.each do |claim| %>
      <tr>
        <td><%= claim[:source] %></td>
        <td><%= claim[:status] %></td>
        <td><%= claim[:id] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
