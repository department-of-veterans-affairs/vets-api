  "type": "object",
  "required": ["data","meta"],
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "$ref": "debt_record.json"
      }
    },
    "meta": {
      "type": "object",
      "required": ["hasDependentDebts"],
      "properties": {
        "hasDependentDebts": { "type": "boolean" }
      }
    }
  }
}