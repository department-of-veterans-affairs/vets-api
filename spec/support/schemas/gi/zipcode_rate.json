{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["id", "type", "attributes"],
      "properties": {
        "id": { "type": "string" },
        "type": { "enum": ["zipcode_rates"] },
        "attributes": {
          "type": "object",
          "required": [
            "zip_code",
            "mha_code",
            "mha_name",
            "mha_rate",
            "mha_rate_grandfathered"],
          "properties": {
            "zip_code": { "type": "string" },
            "mha_code": { "type": ["string", "null"] },
            "mha_name": { "type": ["string", "null"] },
            "mha_rate": { "type": ["number", "null"] },
            "mha_rate_grandfathered": { "type": ["number", "null"] }
          }
        }
      }
    }
  }
}
