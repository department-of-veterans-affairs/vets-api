{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "triageTeamId",
    "name",
    "stationNumber",
    "blockedStatus",
    "preferredTeam",
    "relationType"
  ],
  "properties": {
    "triageTeamId": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "stationNumber": {
      "type": "string"
    },
    "blockedStatus": {
      "type": "boolean"
    },
    "preferredTeam": {
      "type": "boolean"
    },
    "relationType": {
      "type": "string"
    }
  }
}
