{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "type" : "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "required": ["id", "type", "attributes"],
        "properties": {
          "id": { "type": "string"},
          "type": { "enum": ["disability_claims"] },
          "attributes": {
            "type": "object",
            "required": ["evss_id", "date_filed", "min_est_date", "max_est_date", "open", "waiver_submitted", "phase_change_date"],
            "properties": {
              "evss_id": { "type": "integer" },
              "date_filed": { "type": "string" },
              "min_est_date": { "type": ["string", "null"] },
              "max_est_date": { "type": ["string", "null"] },
              "phase_change_date": { "type": ["string", "null"] },
              "open": { "type": "boolean" },
              "waiver_submitted": { "type": "boolean" }
            }
          }
        }
      }
    }
  }
}
