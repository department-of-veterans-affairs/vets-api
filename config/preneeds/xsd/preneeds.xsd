	<xs:complexType name="claimant">
		<xs:sequence>
			<xs:element name="address" type="tns:address" minOccurs="1" />
			<xs:element name="dateOfBirth" type="xs:date" />
			<xs:element name="desiredCemetery" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="\d{3}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="email" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[a-zA-Z0-9_.+-]+@[a-zA-Z0-9_+-]+\.[a-zA-Z]+" />
						<xs:maxLength value="20"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" type="tns:name" />
			<xs:element name="phoneNumber" minOccurs="0">
			<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9+\s-]{0,20}" />
					</xs:restriction>
					</xs:simpleType>
			</xs:element>
			<xs:element name="relationshipToVet">
				<!-- 1 = Veteran/Self, 2 = Spouse/Surviving Spouse, 3 = Unmarried Adult
					Child -->
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ssn">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{3}-\d{2}-\d{4}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="currentlyBuried">
		<xs:sequence>
			<xs:element name="cemeteryNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="\d{3}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" type="tns:name" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="veteran">
		<xs:sequence>
			<xs:element name="address" type="tns:address" minOccurs="0" />
			<xs:element name="currentName" type="tns:name" />
			<xs:element name="dateOfBirth" type="xs:date" minOccurs="0" />
			<xs:element name="dateOfDeath" type="xs:date" minOccurs="0" />
			<xs:element name="gender">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Male" />
						<xs:enumeration value="Female" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isDeceased">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="yes" />
						<xs:enumeration value="no" />
						<xs:enumeration value="unsure" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="maritalStatus">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Single" />
						<xs:enumeration value="Separated" />
						<xs:enumeration value="Married" />
						<xs:enumeration value="Divorced" />
						<xs:enumeration value="Widowed" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="militaryServiceNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="9" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="placeOfBirth" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="serviceName" type="tns:name" />
			<xs:element name="serviceRecords" type="tns:militaryService"
				maxOccurs="1000" />
			<xs:element name="ssn">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{3}-\d{2}-\d{4}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="vaClaimNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="9" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="militaryStatus" minOccurs="1">
			<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--
							ACTIVE_DUTY("A", "ACTIVE DUTY"),
						    RETIRED("R", "RETIRED"),
						    RESERVE_NATIONAL_GUARD("S", "RESERVE/NATIONAL GUARD"),
						    VETERAN("V", "VETERAN"),
						    OTHER_UNKNOWN("X", "OTHER/UNKNOWN"),
						    OTHER("X", "OTHER"),
						    RETIRED_ACTIVE_DUTY("E", "RETIRED ACTIVE DUTY"),
						    DIED_ON_ACTIVE_DUTY("D", "DIED ON ACTIVE DUTY"),
						    RETIRED_RESERVE_NATIONAL_GUARD("O", "RETIRED RESERVE OR NATIONAL GUARD"),
						    DEATH_INACTIVE_DUTY("I", "DEATH RELATED TO INACTIVE DUTY TRAINING");
						 -->
						<xs:enumeration value="A" />
						<xs:enumeration value="R" />
						<xs:enumeration value="S" />
						<xs:enumeration value="V" />
						<xs:enumeration value="X" />
						<xs:enumeration value="E" />
						<xs:enumeration value="D" />
						<xs:enumeration value="O" />
						<xs:enumeration value="I" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="militaryService">
		<xs:sequence>
			<xs:element name="branchOfService" type="tns:branchOfServiceCode" />
			<xs:element name="dischargeType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--
							1 - Honorable
							2 - General
							3 - Entry Level Separation/Uncharacterized
							4 - Other Than Honorable
							5 - Bad Conduct Discharge
							6 - Dishonorable
							7 - Other
						 -->
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
						<xs:enumeration value="5" />
						<xs:enumeration value="6" />
						<xs:enumeration value="7" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="enteredOnDutyDate" type="xs:date"
				minOccurs="0" />
			<xs:element name="highestRank" type="xs:string" minOccurs="0" />
			<xs:element name="nationalGuardState" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="releaseFromDutyDate" type="xs:date"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="receivePreNeedApplicationResponse">
		<xs:sequence>
			<xs:element name="return" type="tns:preNeedResponse"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="preNeedResponse">
		<xs:sequence>
			<xs:element name="applicationUUID" type="xs:string"
				minOccurs="0" />
			<xs:element name="returnCode" type="xs:string" minOccurs="1" />
			<xs:element name="returnDescription" type="xs:string"
				minOccurs="1" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="addAttachment">
		<xs:sequence>
			<xs:element name="trackingNumber">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="20" />
				</xs:restriction>
			</xs:simpleType>
			</xs:element>

			<xs:element name="preNeedAttachment" type="tns:preNeedAttachment"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="addAttachmentResponse">
		<xs:sequence>
			<xs:element name="return" type="tns:preNeedResponse"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="branchOfServiceCode">
		<xs:restriction base="xs:string">
			<xs:length value="2"></xs:length>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
